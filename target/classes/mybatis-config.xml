<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- NULL값 셋팅 -->
	<settings>
		<setting name="cacheEnabled" value="false"/>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	<!-- 별칭 -->
	<typeAliases>
		<typeAlias alias="BoardDto" type="com.green.board.domain.BoardDto" />
		<typeAlias alias="SearchCondition" type="com.green.board.domain.SearchCondition" />
		<typeAlias alias="CommentsDto" type="com.green.board.domain.CommentsDto" />
		<typeAlias alias="UserDto" type="com.green.user.domain.UserDto" />
	</typeAliases>
</configuration>